    "LogLevel": {
      "Default": "Information",
      "Microsoft.SemanticKernel": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "OpenAI": {
    "Source": "OpenAI", // or "AzureOpenAI"
